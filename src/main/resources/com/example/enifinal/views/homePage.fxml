            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.enifinal.controllers.HomePageController">

    <Pane prefHeight="200.0" prefWidth="966.0" style="-fx-background-color: #1E90FF;">
        <Label layoutX="61.0" layoutY="66.0" prefHeight="57.0" prefWidth="222.0" text="Better Ride" textFill="WHITE">
            <font>
                <Font name="Curlz MT" size="36.0"/>
