<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<Pane fx:id="root" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/18"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.enifinal.controllers.FirstPageController"
      maxHeight="520" maxWidth="370"
      minHeight="520" minWidth="370"
      prefHeight="520.0" prefWidth="370.0">

    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label id="Text" layoutX="45.0" layoutY="447.0" stylesheets="@zdesign.css"
           text="Already have an account? "/>
    <ImageView fitHeight="184.0" fitWidth="183.0" layoutX="87.0" layoutY="91.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../img/logo_and_stuff/hi-robot.gif"/>
    </ImageView>
    <Label alignment="CENTER" layoutX="45.0" layoutY="260.0" prefHeight="44.0" prefWidth="281.0"
           stylesheets="@zdesign.css" text="Welcome" textFill="#27496d">
        <font>
            <Font size="36.0"/>
        </font>
    </Label>
    <Button id="Button" layoutX="45.0" layoutY="363.0" maxHeight="-Infinity" maxWidth="-Infinity"
            onAction="#signup_btn"
            minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="44.0" prefWidth="291.0"
            stylesheets="@zdesign.css" text="Getting Started" textFill="WHITE"/>

    <Label id="Text" layoutX="49.0" layoutY="319.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity" prefHeight="26.0" prefWidth="233.0" stylesheets="@zdesign.css"/>

    <ImageView fitHeight="123.0" fitWidth="118.0" layoutX="120.0" layoutY="0.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../img/logo_and_stuff/logo.jpg"/>
    </ImageView>
    <Hyperlink id="Text" layoutX="211.0" layoutY="444.0" style="-fx-underline: true;"
               stylesheets="@zdesign.css" text="Log In" textFill="#27496d" onAction="#login_btn"/>
    <Label id="Text" layoutX="45.0" layoutY="319.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity" prefHeight="26.0" prefWidth="291.0" stylesheets="@zdesign.css"
           text="Create an account and search for new cars..."/>
</Pane>
